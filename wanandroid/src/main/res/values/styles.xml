    <style name="AppTheme" parent="Theme.MaterialComponents.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/color333333</item>
        <item name="colorPrimaryDark">@color/color333333</item>
        <item name="colorAccent">@color/color333333</item>
